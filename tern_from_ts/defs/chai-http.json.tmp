    },
    "ChaiHttpRequest": {
      "agent": "fn(server: ?) -> ChaiHttp.Agent",
      "addPromises": "fn(promiseConstructor: ?)"
    },
    "Assertion": {
      "status": "fn(code: number) -> Assertion",
      "header": "fn(key: string, value?: string) -> Assertion",
