{
  "JScrollPaneSettings": {
    "showArrows": "bool",
    "maintainPosition": "bool",
    "stickToBottom": "bool",
    "stickToRight": "bool",
    "autoReinitialise": "bool",
    "autoReinitialiseDelay": "number",
    "verticalDragMinHeight": "number",
    "verticalDragMaxHeight": "number",
    "horizontalDragMinWidth": "number",
    "horizontalDragMaxWidth": "number",
    "contentWidth": "number",
    "animateScroll": "bool",
    "animateDuration": "number",
    "animateEase": "string",
    "hijackInternalLinks": "bool",
    "verticalGutter": "number",
    "horizontalGutter": "number",
    "mouseWheelSpeed": "number",
    "arrowButtonSpeed": "number",
    "arrowRepeatFreq": "number",
    "arrowScrollOnHover": "bool",
    "verticalArrowPositions": "string",
    "horizontalArrowPositions": "string",
    "enableKeyboardNavigation": "bool",
    "hideFocus": "bool",
    "clickOnTrack": "bool",
    "trackClickSpeed": "number",
    "trackClickRepeatFreq": "number"
  },
  "JScrollPaneApi": {
    "reinitialise": "fn(options?: JScrollPaneSettings)",
    "scrollToElement": "fn(ele: JQuery, stickToTop?: bool, animate?: bool)",
    "scrollTo": "fn(destX: number, destY: number, animate?: bool)",
    "scrollToX": "fn(destX: number, animate?: bool)",
    "scrollToY": "fn(destY: number, animate?: bool)",
    "scrollToPercentX": "fn(destPercentX: number, animate?: bool)",
    "scrollToPercentY": "fn(destPercentY: number, animate?: bool)",
    "scrollBy": "fn(deltaX: number, deltaY: number, animate?: bool)",
    "scrollByX": "fn(deltaX: number, animate?: bool)",
    "scrollByY": "fn(deltaY: number, animate?: bool)",
    "positionDragX": "fn(x: number, animate?: bool)",
    "positionDragY": "fn(y: number, animate?: bool)",
    "animate": "fn(ele: JQuery, prop: string, value: ?, stepCallback: fn(args: [?]))",
    "getContentPositionX": "fn() -> number",
    "getContentPositionY": "fn() -> number",
    "getContentWidth": "fn() -> number",
    "getContentHeight": "fn() -> number",
    "getIsScrollableH": "fn() -> bool",
    "getPercentScrolledX": "fn() -> number",
    "getPercentScrolledY": "fn() -> number",
    "getIsScrollableV": "fn() -> bool",
    "getContentPane": "fn() -> JQuery",
    "scrollToBottom": "fn(animate?: bool)",
    "hijackInternalLinks": "fn()",
    "destroy": "fn()"
  },
  "JQuery": {
    "jScrollPane": "fn(options?: JScrollPaneSettings) -> JQuery"
  },
  "!name": "jscrollpane"
}
