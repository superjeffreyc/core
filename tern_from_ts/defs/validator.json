{
  "IURLoptions": {
    "protocols": "[string]",
    "require_tld": "bool",
    "require_protocol": "bool",
    "allow_underscores": "bool"
  },
  "IFQDNoptions": {
    "require_tld": "bool",
    "allow_underscores": "bool"
  },
  "IEmailoptions": {
    "lowercase": "bool"
  },
  "IExtendCallback": {},
  "IExtendFunc": {},
  "IValidatorStatic": {
    "extend": "fn(name: string, fn: IExtendCallback) -> IExtendFunc",
    "equals": "fn(str: string, comparison: ?) -> bool",
    "contains": "fn(str: string, elem: ?) -> bool",
    "matches": "fn(str: string, pattern: ?, modifiers?: string) -> bool",
    "isEmail": "fn(str: string) -> bool",
    "isURL": "fn(str: string, options?: IURLoptions) -> bool",
    "isFQDN": "fn(str: string, options?: IFQDNoptions) -> bool",
    "isMACAddress": "fn(str: string) -> bool",
    "isIP": "fn(str: string, version?: number) -> bool",
    "isAlpha": "fn(str: string) -> bool",
    "isNumeric": "fn(str: string) -> bool",
    "isAlphanumeric": "fn(str: string) -> bool",
    "isBase64": "fn(str: string) -> bool",
    "isHexadecimal": "fn(str: string) -> bool",
    "isHexColor": "fn(str: string) -> bool",
    "isLowercase": "fn(str: string) -> bool",
    "isUppercase": "fn(str: string) -> bool",
    "isInt": "fn(str: string) -> bool",
    "isFloat": "fn(str: string) -> bool",
    "isDivisibleBy": "fn(str: string, number: number) -> bool",
    "isNull": "fn(str: string) -> bool",
    "isLength": "fn(str: string, min: number, max?: number) -> bool",
    "isByteLength": "fn(str: string, min: number, max?: number) -> bool",
    "isUUID": "fn(str: string, version?: number) -> bool",
    "isDate": "fn(str: string) -> bool",
    "isAfter": "fn(str: string, date?: Date) -> bool",
    "isBefore": "fn(str: string, date?: Date) -> bool",
    "isIn": "fn(str: string, values: [?]) -> bool",
    "isCreditCard": "fn(str: string) -> bool",
    "isISBN": "fn(str: string, version?: number) -> bool",
    "isJSON": "fn(str: string) -> bool",
    "isMultibyte": "fn(str: string) -> bool",
    "isAscii": "fn(str: string) -> bool",
    "isFullWidth": "fn(str: string) -> bool",
    "isHalfWidth": "fn(str: string) -> bool",
    "isVariableWidth": "fn(str: string) -> bool",
    "isSurrogatePair": "fn(str: string) -> bool",
    "isMongoId": "fn(str: string) -> bool",
    "toDate": "fn(input: ?) -> ?",
    "toFloat": "fn(input: ?) -> number",
    "toInt": "fn(input: ?, radix?: number) -> number",
    "toBoolean": "fn(input: ?, strict?: bool) -> bool",
    "trim": "fn(input: ?, chars?: string) -> string",
    "ltrim": "fn(input: ?, chars?: string) -> string",
    "rtrim": "fn(input: ?, chars?: string) -> string",
    "escape": "fn(input: string) -> string",
    "stripLow": "fn(input: string, keep_new_lines?: bool) -> string",
    "whitelist": "fn(input: string, chars: string) -> string",
    "blacklist": "fn(input: string, chars: string) -> string",
    "normalizeEmail": "fn(email: string, options?: IEmailoptions) -> string"
  },
  "validator": "validator",
  "!name": "validator"
}
